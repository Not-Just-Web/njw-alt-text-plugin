{
  "name": "njw-alt-text-plugin",
  "version": "1.2.0",
  "description": "A wordpress plugin to auto generate the Alt text",
  "scripts": {
    "preinstall": "composer install",
    "prepare": "git config core.hooksPath ./git-hooks",
    "test": "npm-run-all --parallel test:*",
    "test:unit-php": "composer test",
    "test:unit-js": "wp-scripts test-unit-js --verbose",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "wp-scripts lint-style",
    "lint:php": "composer lint",
    "lint:ts": "wp-scripts lint-js  --ext .ts,.tsx",
    "format": "npm-run-all --parallel format:*",
    "format:css": "npm run lint:css -- --fix",
    "format:ts": "wp-scripts lint-js --ext .ts,.tsx  --fix",
    "sync:install": "brew install fswatch",
    "sync": "./sync.sh",
    "format:php": "composer format",
    "php": "composer format",
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "config:git-hooks": "git config core.hooksPath ./git-hooks",
    "create:block": "cd ./src/blocks && npx @wordpress/create-block --no-plugin --template njw-wp-typescript-block"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@wordpress/block-editor": "^12.23.0",
    "@wordpress/blocks": "^12.32.0",
    "@wordpress/components": "^27.3.0",
    "@wordpress/data": "^9.25.0",
    "antd": "^5.16.1",
    "html-entities": "^2.5.2",
    "mini-css-extract-plugin": "^2.8.1",
    "papaparse": "^5.4.1",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/papaparse": "^5.3.14",
    "@types/wordpress__block-editor": "^11.5.14",
    "@types/wordpress__blocks": "^12.5.14",
    "@typescript-eslint/parser": "^6.21.0",
    "@wordpress/create-block": "^4.39.0",
    "@wordpress/scripts": "^27.6.0",
    "fs": "^0.0.1-security",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "stylelint-use-logical": "^2.1.2",
    "ts-loader": "^9.5.1"
  }
}
